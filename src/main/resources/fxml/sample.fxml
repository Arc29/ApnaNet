      <TextField fx:id="nameField" prefHeight="41.0" prefWidth="360.0" promptText="Enter name">
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="emailField" prefHeight="43.0" prefWidth="354.0" promptText="Enter email">
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin>
      </TextField>
      <PasswordField fx:id="passField" prefHeight="36.0" prefWidth="359.0" promptText="Enter password">
         <VBox.margin>
            <Insets bottom="30.0" />
         </VBox.margin></PasswordField>
      <Button fx:id="registerBtn" mnemonicParsing="false" onAction="#buttonClicked" text="Register">
         <VBox.margin>
            <Insets left="120.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="fileBtn" mnemonicParsing="false" onAction="#selectFile" text="Add file" />
