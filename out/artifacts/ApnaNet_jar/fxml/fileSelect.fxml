<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane fx:id="mainPane" prefHeight="410.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.FileController">
   <children>
      <TextField fx:id="fchooseTxt" editable="false" layoutX="90.0" layoutY="57.0" prefHeight="26.0" prefWidth="288.0" promptText="Choose File..." />
      <Button fx:id="fchooseBtn" layoutX="459.0" layoutY="57.0" mnemonicParsing="false" onAction="#fChoose" text="Select File" />
      <TextField fx:id="nameTxt" layoutX="90.0" layoutY="126.0" prefHeight="26.0" prefWidth="288.0" promptText="Enter name" />
      <ComboBox fx:id="catBox" layoutX="93.0" layoutY="194.0" prefHeight="26.0" prefWidth="207.0" promptText="Select category.." />
      <TextField fx:id="tagsTxt" layoutX="90.0" layoutY="264.0" prefHeight="26.0" prefWidth="288.0" promptText="Enter tags (seperate by ;)" />
      <Button fx:id="shareBtn" layoutX="234.0" layoutY="321.0" mnemonicParsing="false" onAction="#share" text="Share" />
   </children>
</AnchorPane>
